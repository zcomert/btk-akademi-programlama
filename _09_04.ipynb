   "source": [
    "sayilar = []\n",
    "\n",
    "for i in range(5):\n",
    "    s = int(input(\"Bir sayı giriniz: \"))\n",
    "    sayilar.append(s)\n",
    "    \n",
    "toplam = 0\n",
    "for sayi in sayilar:\n",
    "    toplam += sayi\n",
    "\n",
    "ortalama = toplam / len(sayilar)\n",
    "print(f\"Girilen sayıların ortalaması: {ortalama:.2f}\")"
   ]
